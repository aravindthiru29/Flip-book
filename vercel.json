{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python@3.8"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "PYTHONDONTWRITEBYTECODE": "1"
    }
}